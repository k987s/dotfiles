set encoding=utf-8
set fileencoding=utf-8
set fileencodings=utf-8,sjis,cp932
set fileformats=unix,dos,mac

set nobackup
set noswapfile

colorscheme material
set t_Co=256

set cursorline
set number
set showmatch

set list
set listchars=tab:~.,trail:-

set nowrap

set expandtab
set tabstop=2
set shiftwidth=2
